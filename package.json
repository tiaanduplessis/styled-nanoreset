{
  "name": "styled-nanoreset",
  "description": "Nanoreset for Styled Components",
  "version": "1.0.1",
  "author": "Tiaan du Plessis <tiaanduplessis@hotmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node inject.js && npm run clean && babel src/ -d dist/",
    "clean": "rm -rf dist && mkdir -p dist",
    "postbuild": "flow-copy-source src dist",
    "prebuild": "npm t",
    "preversion": "npm run build",
    "test": "eslint -c .eslintrc.json src"
  },
  "homepage": "https://github.com/tiaanduplessis/styled-nanoreset#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/tiaanduplessis/styled-nanoreset.git"
  },
  "bugs": "https://github.com/tiaanduplessis/styled-nanoreset/issues",
  "keywords": [
    "css",
    "reset",
    "nanoreset",
    "styled-components"
  ],
  "dependencies": {
    "nanoreset": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "7.7.0",
    "@babel/core": "7.7.2",
    "@babel/plugin-transform-modules-commonjs": "7.7.0",
    "@babel/preset-env": "7.7.1",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.3.0",
    "eslint-config-nitpicky": "^4.0.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-promise": "^4.2.1",
    "flow-copy-source": "2.0.2",
    "prettier": "^1.18.2",
    "styled-components": "4.3.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "peerDependencies": {
    "styled-components": ">=3.0.0 || >=4.0.0"
  }
}
