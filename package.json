{
  "name": "styled-nanoreset",
  "description": "Nanoreset for Styled Components",
  "version": "1.0.0",
  "author": "Tiaan du Plessis <tiaanduplessis@hotmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node inject.js && npm run clean && babel src/ -d dist/",
    "clean": "rm -rf dist && mkdir -p dist",
    "postbuild": "flow-copy-source src dist",
    "prebuild": "npm t",
    "preversion": "npm run build",
    "test": "eslint -c .eslintrc.json src"
  },
  "homepage": "https://github.com/tiaanduplessis/styled-nanoreset#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/tiaanduplessis/styled-nanoreset.git"
  },
  "bugs": "https://github.com/tiaanduplessis/styled-nanoreset/issues",
  "keywords": [
    "css",
    "reset",
    "nanoreset",
    "styled-components"
  ],
  "dependencies": {
    "nanoreset": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/plugin-transform-modules-commonjs": "7.2.0",
    "@babel/preset-env": "7.2.3",
    "babel-eslint": "10.0.1",
    "eslint": "5.11.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-nitpicky": "3.2.1",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-compat": "2.6.3",
    "eslint-plugin-flowtype": "3.2.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jest": "22.1.2",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.11.1",
    "flow-copy-source": "2.0.2",
    "styled-components": "4.1.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "peerDependencies": {
    "styled-components": ">=3.0.0 || >=4.0.0"
  }
}
